{
  "name": "rin-linin-art-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run env && ng serve --port 4400",
    "build": "npm run env && ng build",
    "build:production": "npm run env && ng build -c=production && cp crawler_prod/* dist/rin-linin-art-showcase/browser/",
    "build:preview": "npm run env && ng build -c=development && cp crawler_test/* dist/rin-linin-art-showcase/browser/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "env": "node -p \"'export const npm_package_version = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/environments/.env.ts",
    "serve:ssr:rin-linin-art-showcase": "node dist/rin-linin-art-showcase/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xss": "^1.0.15",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/localize": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.4"
  }
}
