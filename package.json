{
  "name": "rin-linin-art-showcase",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run env && ng serve --port 4400",
    "build": "npm run env && ng build",
    "build:production": "npm run env && ng build -c=production && npm run sitemap && cp crawler_prod/* dist/rin-linin-art-showcase/browser/",
    "build:preview": "npm run env && ng build -c=development && cp crawler_test/* dist/rin-linin-art-showcase/browser/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "env": "node -p \"'export const npm_package_version = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/environments/.env.ts",
    "serve:ssr:rin-linin-art-showcase": "node dist/rin-linin-art-showcase/server/server.mjs",
    "sitemap": "node ./sitemaps/generate-sitemap.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.3",
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^19.2.3",
    "@angular/compiler": "^19.2.3",
    "@angular/core": "^19.2.3",
    "@angular/forms": "^19.2.3",
    "@angular/material": "^18.2.0",
    "@angular/platform-browser": "^19.2.3",
    "@angular/platform-browser-dynamic": "^19.2.3",
    "@angular/platform-server": "^19.2.3",
    "@angular/router": "^19.2.3",
    "@angular/ssr": "^19.2.4",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/postcss": "^4.1.7",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.7",
    "tslib": "^2.3.0",
    "xss": "^1.0.15",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.3",
    "@angular/localize": "^19.2.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sitemap": "^8.0.0",
    "typescript": "~5.5.4"
  }
}
